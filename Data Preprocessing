   "source": [
    "def SoccerNet_DataPreprocessing(root_dir, img_width=1920, img_height=1080):\n",
    "    \"\"\"\n",
    "    Function to process data\n",
    "    \n",
    "    Arguments:\n",
    "    root_dir (str): Path to the root directory containing game folders.\n",
    "    img_width (int): Width of the images for normalization.\n",
    "    img_height (int): Height of the images for normalization.\n",
    "    \n",
    "    Returns:\n",
    "    game_info (list): List containing game metadata.\n",
    "    image_data (list): List containing image metadata and annotations.\n",
    "    game_df (DataFrame): DataFrame containing the game metadata.\n",
    "    image_df (DataFrame): DataFrame containing the image metadata and annotations.\n",
    "    \"\"\"\n",
    "    \n",
    "    # Initialize the lists to store game and image data\n",
    "    game_info = []\n",
    "    image_data = []\n",
    "\n",
    "    # Loop through game folders\n",
    "    for train_folder in os.listdir(root_dir):\n",
    "        train_folder_path = os.path.join(root_dir, train_folder)\n",
    "\n",
    "        # Check if it's a directory (game folder)\n",
    "        if os.path.isdir(train_folder_path):\n",
    "            label_path = os.path.join(train_folder_path, 'Labels-GameState.json')\n",
    "            image_folder_path = os.path.join(train_folder_path, 'img1')\n",
    "            \n",
    "            # Check if label file exists\n",
    "            if os.path.exists(label_path):\n",
    "                with open(label_path, 'r') as label_file:\n",
    "                    # Label Keys: info, images, annotations, categories\n",
    "                    label_data = json.load(label_file)\n",
    "\n",
    "                    # Append game metadata\n",
    "                    game_info.append(\n",
    "                        {\n",
    "                            'Game Version': label_data['info']['version'],\n",
    "                            'Game ID': label_data['info']['game_id'],\n",
    "                            'Game Unique ID': label_data['info']['id'],\n",
    "                            'Tracklets Count': label_data['info']['num_tracklets'],\n",
    "                            'Action Position': label_data['info']['action_position'],\n",
    "                            'Action Class': label_data['info']['action_class'],\n",
    "                            'Game Folder Path': train_folder_path,\n",
    "                            'Image Folder Path': image_folder_path\n",
    "                        }\n",
    "                    )\n",
    "\n",
    "                    # Extract image IDs\n",
    "                    image_ids = [img['image_id'] for img in label_data['images']]\n",
    "\n",
    "                    # Loop through image IDs and store their metadata\n",
    "                    for img_id in image_ids:\n",
    "                        img_info = {}\n",
    "\n",
    "                        # Find corresponding image data\n",
    "                        img_data = next((img for img in label_data['images'] if img['image_id'] == img_id), None)\n",
    "                        if img_data:\n",
    "                            img_info['Image ID'] = img_data['image_id']\n",
    "                            img_info['File Name'] = img_data['file_name']\n",
    "                            img_info['Image Path'] = os.path.join(image_folder_path, img_data['file_name'])\n",
    "\n",
    "                            # Extract annotations (bounding boxes, etc.)\n",
    "                            annotations = []\n",
    "                            for annotation in label_data['annotations']:\n",
    "                                if annotation['image_id'] == img_id:\n",
    "                                    # Normalize Bounding Box (Image)\n",
    "                                    bbox_image = annotation.get('bbox_image', {})\n",
    "                                    bbox_normalized = {\n",
    "                                        'x': bbox_image.get('x', 0) / img_width,\n",
    "                                        'y': bbox_image.get('y', 0) / img_height,\n",
    "                                        'x_center': bbox_image.get('x_center', 0) / img_width,\n",
    "                                        'y_center': bbox_image.get('y_center', 0) / img_height,\n",
    "                                        'w': bbox_image.get('w', 0) / img_width,\n",
    "                                        'h': bbox_image.get('h', 0) / img_height\n",
    "                                    }\n",
    "\n",
    "                                    # Check if bbox_pitch exists and normalize\n",
    "                                    bbox_pitch = annotation.get('bbox_pitch', None)\n",
    "                                    if bbox_pitch is not None:\n",
    "                                        bbox_pitch_normalized = {\n",
    "                                            'x_bottom_left': bbox_pitch.get('x_bottom_left', 0),\n",
    "                                            'y_bottom_left': bbox_pitch.get('y_bottom_left', 0),\n",
    "                                            'x_bottom_right': bbox_pitch.get('x_bottom_right', 0),\n",
    "                                            'y_bottom_right': bbox_pitch.get('y_bottom_right', 0),\n",
    "                                            'x_bottom_middle': bbox_pitch.get('x_bottom_middle', 0),\n",
    "                                            'y_bottom_middle': bbox_pitch.get('y_bottom_middle', 0)\n",
    "                                        }\n",
    "                                    else:\n",
    "                                        bbox_pitch_normalized = {}\n",
    "\n",
    "                                    # Check if bbox_pitch_raw exists and normalize\n",
    "                                    bbox_pitch_raw = annotation.get('bbox_pitch_raw', None)\n",
    "                                    if bbox_pitch_raw is not None:\n",
    "                                        bbox_pitch_raw_normalized = {\n",
    "                                            'x_bottom_left': bbox_pitch_raw.get('x_bottom_left', 0),\n",
    "                                            'y_bottom_left': bbox_pitch_raw.get('y_bottom_left', 0),\n",
    "                                            'x_bottom_right': bbox_pitch_raw.get('x_bottom_right', 0),\n",
    "                                            'y_bottom_right': bbox_pitch_raw.get('y_bottom_right', 0),\n",
    "                                            'x_bottom_middle': bbox_pitch_raw.get('x_bottom_middle', 0),\n",
    "                                            'y_bottom_middle': bbox_pitch_raw.get('y_bottom_middle', 0)\n",
    "                                        }\n",
    "                                    else:\n",
    "                                        bbox_pitch_raw_normalized = {}\n",
    "\n",
    "                                    # Store normalized values in annotations\n",
    "                                    annotations.append({\n",
    "                                        'Track ID': annotation.get('track_id', None),  # Safely access the 'track_id'\n",
    "                                        'Supercategory': annotation.get('supercategory', None),\n",
    "                                        'Category ID': annotation.get('category_id', None),\n",
    "                                        'Role': annotation['attributes'].get('role', None) if 'attributes' in annotation else None,\n",
    "                                        'Jersey': annotation['attributes'].get('jersey', None) if 'attributes' in annotation else None,\n",
    "                                        'Team': annotation['attributes'].get('team', None) if 'attributes' in annotation else None,\n",
    "                                        'Bounding Box (Image)': bbox_normalized,\n",
    "                                        'Bounding Box (Pitch)': bbox_pitch_normalized,\n",
    "                                        'Bounding Box (Raw Pitch)': bbox_pitch_raw_normalized\n",
    "                                    })\n",
    "\n",
    "                            img_info['Annotations'] = annotations\n",
    "                            image_data.append(img_info)\n",
    "\n",
    "    # Convert game info and image info to DataFrame for easy inspection\n",
    "    game_df = pd.DataFrame(game_info)\n",
    "    image_df = pd.DataFrame(image_data)\n",
    "\n",
    "    return game_info, image_data, game_df, image_df"
   ]
  },
